#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/srv/http/P_I_M_P/node_modules/.pnpm/typescript@file+..+..+..+home+shyline+projects+SOURCES+LIBS+typescript-5.8.3.tgz_b7d3c904e5bce9dfeae141ecca980237/node_modules/typescript/bin/node_modules:/srv/http/P_I_M_P/node_modules/.pnpm/typescript@file+..+..+..+home+shyline+projects+SOURCES+LIBS+typescript-5.8.3.tgz_b7d3c904e5bce9dfeae141ecca980237/node_modules/typescript/node_modules:/srv/http/P_I_M_P/node_modules/.pnpm/typescript@file+..+..+..+home+shyline+projects+SOURCES+LIBS+typescript-5.8.3.tgz_b7d3c904e5bce9dfeae141ecca980237/node_modules:/srv/http/P_I_M_P/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/srv/http/P_I_M_P/node_modules/.pnpm/typescript@file+..+..+..+home+shyline+projects+SOURCES+LIBS+typescript-5.8.3.tgz_b7d3c904e5bce9dfeae141ecca980237/node_modules/typescript/bin/node_modules:/srv/http/P_I_M_P/node_modules/.pnpm/typescript@file+..+..+..+home+shyline+projects+SOURCES+LIBS+typescript-5.8.3.tgz_b7d3c904e5bce9dfeae141ecca980237/node_modules/typescript/node_modules:/srv/http/P_I_M_P/node_modules/.pnpm/typescript@file+..+..+..+home+shyline+projects+SOURCES+LIBS+typescript-5.8.3.tgz_b7d3c904e5bce9dfeae141ecca980237/node_modules:/srv/http/P_I_M_P/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsc" "$@"
else
  exec node  "$basedir/../../bin/tsc" "$@"
fi
