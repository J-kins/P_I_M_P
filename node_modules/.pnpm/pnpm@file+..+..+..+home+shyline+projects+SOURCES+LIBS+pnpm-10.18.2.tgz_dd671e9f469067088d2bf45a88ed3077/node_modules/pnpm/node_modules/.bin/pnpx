#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/srv/http/P_I_M_P/node_modules/.pnpm/pnpm@file+..+..+..+home+shyline+projects+SOURCES+LIBS+pnpm-10.18.2.tgz_dd671e9f469067088d2bf45a88ed3077/node_modules/pnpm/bin/node_modules:/srv/http/P_I_M_P/node_modules/.pnpm/pnpm@file+..+..+..+home+shyline+projects+SOURCES+LIBS+pnpm-10.18.2.tgz_dd671e9f469067088d2bf45a88ed3077/node_modules/pnpm/node_modules:/srv/http/P_I_M_P/node_modules/.pnpm/pnpm@file+..+..+..+home+shyline+projects+SOURCES+LIBS+pnpm-10.18.2.tgz_dd671e9f469067088d2bf45a88ed3077/node_modules:/srv/http/P_I_M_P/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/srv/http/P_I_M_P/node_modules/.pnpm/pnpm@file+..+..+..+home+shyline+projects+SOURCES+LIBS+pnpm-10.18.2.tgz_dd671e9f469067088d2bf45a88ed3077/node_modules/pnpm/bin/node_modules:/srv/http/P_I_M_P/node_modules/.pnpm/pnpm@file+..+..+..+home+shyline+projects+SOURCES+LIBS+pnpm-10.18.2.tgz_dd671e9f469067088d2bf45a88ed3077/node_modules/pnpm/node_modules:/srv/http/P_I_M_P/node_modules/.pnpm/pnpm@file+..+..+..+home+shyline+projects+SOURCES+LIBS+pnpm-10.18.2.tgz_dd671e9f469067088d2bf45a88ed3077/node_modules:/srv/http/P_I_M_P/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/pnpx.cjs" "$@"
else
  exec node  "$basedir/../../bin/pnpx.cjs" "$@"
fi
