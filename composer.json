{
    "name": "groupe/pimp",
    "description": "P.I.M.P - Business Repository Platform based on Trust",
    "type": "project",
    "autoload": {
        "psr-4": {
            "PIMP\\": "",
            "PIMP\\Core\\": "service/",
            "PIMP\\Controllers\\": "controller/",
            "PIMP\\Models\\": "model/",
            "PIMP\\Services\\": "service/",
            "PIMP\\Services\\Database\\": "service/Database/",
            "PIMP\\Utils\\": "util/",
            "PIMP\\Views\\": "view/",
            "PIMP\\Views\\Pages\\": "view/pages/",
            "PIMP\\Views\\Components\\": "view/components/",
            "PIMP\\Admin\\": "admin/",
            "PIMP\\Admin\\Controllers\\": "admin/controller/",
            "PIMP\\Admin\\Models\\": "admin/model/",
            "PIMP\\Admin\\Services\\": "admin/service/",
            "PIMP\\Admin\\Views\\": "admin/view/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "post-autoload-dump": [
            "PIMP\\Core\\Autoloader::register"
        ]
    }
}